{
  "manifest_version": 3,
  "name": "Leads Tracker",
  "version": "1.0",
  "description": "Save useful links and browser tabs for later reference",

  "action": {
    "default_popup": "index2.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },

  "permissions": ["tabs"]
}
